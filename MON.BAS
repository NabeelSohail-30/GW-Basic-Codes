10 CLS
20 INPUT "ENTER MONTH" ; M
30 IF M = 1 THEN MON$ = "JAN" : GOTO 200
40 IF M = 2 THEN MON$ = "FEB" : GOTO 200
45 IF M = 3 THEN MON$ = "MARCH" : GOTO 200
50 IF M = 4 THEN MON$ = "APRIL" : GOTO 200
60 IF M = 5 THEN MON$ = "MAY" : GOTO 200
70 IF M = 6 THEN MON$ = "JUNE" : GOTO 200
80 IF M = 7 THEN MON$ = "JULY" : GOTO 200
90 IF M = 8 THEN MON$ = "AUG" : GOTO 200
100 IF M = 9 THEN MON$ = "SEP" : GOTO 200
110 IF M = 10 THEN MON$ = "OCT" : GOTO 200
120 IF M = 11 THEN MON$ = "NOV" : GOTO 200
130 IF M = 12 THEN MON$ = "DEC" : GOTO 200
140 IF M <1 OR M >12 THEN PRINT "INVALID MONTH" : GOTO 20
200 PRINT MON$
