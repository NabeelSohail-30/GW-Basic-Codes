10 CLS
20 G$ = "GOOD"
30 D$ = "MORNING,AFTERNOON,EVENING,NIGHT"
40 PRINT "CHOOSE OPTION (A , B . C , D , E)"
42 INPUT ; OP$ : CLS
50 IF OP$ = "A" THEN PRINT G$ ; MID$(D$,1,7):GOTO 110 ELSE 60
60 IF OP$ = "B" THEN PRINT G$ ; MID$(D$,9,9)
70 IF OP$ = "C" THEN PRINT G$ ; MID$(D$,19,7)
80 IF OP$ = "D" THEN PRINT G$ ; MID$(D$,27,5)
90 IF OP$ = "E" THEN GOTO 100 ELSE GOTO 100
100 CLS : PRINT "TRY AGAIN" : GOTO 42
110 END
