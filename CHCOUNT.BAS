10 CLS
20 INPUT "ENTER STRING" ; S$
30 INPUT "ENTER CHARACTER YOU WANT TO COUNT" ; CH$
40 L = LEN(S$)
45 CHCOUNT% = 0
50 FOR P = 1 TO L
60 IF MID$(S$,P,1) = CH$ THEN GOTO 65 ELSE GOTO 70
65 CHCOUNT% = CHCOUNT% + 1
70 NEXT P
90 PRINT "CHARACTER "  ; CH$ ; " OCCURS" ; CHCOUNT% ; "TIMES IN YOUR STRING"
100 END
