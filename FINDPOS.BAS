10 CLS
20 INPUT "ENTER ANY STRING" ; S$
30 INPUT "ENTER CHARACTER TO FIND 1ST POSITION" ; CH$
40 L = LEN(S$)
50 FOR P = 1 TO L
60 IF CH$ = MID$(S$,P,1) THEN GOTO 80 ELSE GOTO 70
70 NEXT P : PRINT "SYSTEM COULD NOT FIND CHARACTER "; CH$ : END
80 PRINT "1ST POSTITION OF CHARACTER " ; CH$ ; " = " ; P
90 END
